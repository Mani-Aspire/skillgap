@{
    ViewBag.Title = "Login";
}
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content( "~/CSS/Bootstrap/bootstrap.css" )" rel="stylesheet" type="text/css" />
    <link href="@Url.Content( "~/CSS/Custom.css" )" rel="stylesheet" type="text/css" />
    <script src="@Url.Content( "~/Scripts/Base/Jquery/jquery-1.10.1.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Base/Jquery/modernizr-1.7.min.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Base/Knockout/knockout-2.3.0.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/ICNamespace.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Assert.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Utils.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Data/Data.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/Startup.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/jquery.jstree.js" )" type="text/javascript"></script>
</head>
<body>
    <div class="container loginContainer">
        <div class="row">
            <div id="login" class="span4 well">
                <form method="post">
                <fieldset>
                    <legend>IConnect Login</legend>
                    <div class="form-group">
                        <label for="exampleInputEmail">
                            User Name</label>
                        <input type="text" class="form-control" id="exampleInputEmail" placeholder="UserName">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword">
                            Password</label>
                        <input type="password" class="form-control" id="exampleInputPassword" placeholder="Password">
                    </div>                                        
                    <button type="submit" class="btn btn-default">
                        Submit</button>
                </fieldset>
                </form>
            </div>
        </div>
    </div>
</body>
</html>
